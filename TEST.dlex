	//suit
	1 2 3 4 5 6 7 8

	0 //oid

	//attribute right
	//lowest low high
	ATTR_READ ATTR_READ ATTR_READ
	ATTR_READ ATTR_READ ATTR_READ ASCII PHASE_N 0 SCALE_ZP VISIBLE_STRING 0
	
	//method right
	//lowest low high
}

